import math

def factors(n):
    return set(reduce(list.__add__,
                ([i, n//i] for i in range(1, int(n**0.5) + 1) if n % i == 0)))

def isPrime(n):
    if n==1:
        return False
    elif n<4:
        return True
    elif n%2==0:
        return False
    elif n<9:
        return True
    elif n%3==0:
        None
    else:
        r = math.floor(math.sqrt(n))
        f =5
        while f<=r:
            if n%f==0:
                return False
            if n%(f+2)==0:
                return False
            f += 6
        return True

n = 600851475143
result = 0
for factor in factors(n):
    if isPrime(factor):
        if factor>result:
            result = factor
print result
